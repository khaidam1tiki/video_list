.header-section-container {
  display: flex; 
  flex-direction: row; 
  justify-content: space-between; 
  position: absolute;
  width: calc(100% - 95px);
  z-index: 100; 
}

.header-icon-container {
  width: 40px;
  height: 40px;
  background-color: rgba(0, 0, 0, 0.4); 
  display: flex; 
  justify-content: center; 
  align-items: center;
  border-radius: 20px;
}