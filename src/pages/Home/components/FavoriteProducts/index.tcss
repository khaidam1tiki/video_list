.favorite-products-section {
  margin-top: 8px;
  padding: 12px 16px;
  background-color: var(--white);
}

.favorite-products-summary {
  font-weight: 400;
  color: var(--gray60);
}

.favorite-products-scrollview::-webkit-scrollbar {
  display: none;
}

.favorite-products-scrollview {
  display: flex; 
  flex-direction: row;
  background-color: var(--white);
  margin-right: -16px;
  margin-left: -16px;
  margin-top: 12px;
  padding-left: 16px;
}